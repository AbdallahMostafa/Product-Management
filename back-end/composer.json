{
    "require": {
        "vlucas/phpdotenv": "^2.4",
        "doctrine/orm": "^2.11.0",
        "doctrine/dbal": "^3.2",
        "symfony/cache": "^5.4",
        "abdallah-mostafa/custom-autoloader-package": "1.0.0",
        "php-di/php-di": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Src\\": "src/"
        }
    },
    "repositories": [
        {
